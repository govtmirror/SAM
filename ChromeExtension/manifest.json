{
  "name" : "Help me with SAM",
  "version" : "0.5.0",
  "description" : "show me what I need to know to register at SAM",
  "background" : { "page": "samHelper/background.html" },
  "permissions" : [
          "storage",
          "tabs",
          "https://www.sam.gov/*",
          "http://127.0.0.1:8124/*",
          "http://mo.tynsax.com/api/*"
  ],
  "page_action": {
    "default_icon": {                  
    "19": "samHelper/icon19.png",          
    "38": "samHelper/icon38.png",
    "48": "samHelper/icon48.png"          
    },
    "default_title": "help with SAM registration",
    "default_popup": "samHelper/popup.html"
    },
  "content_scripts": [
	   {
	   "matches": ["https://www.sam.gov/*"],
	   "css": ["samHelper/samHelper.css", "SamTracker/samtracker.css"],
	   "js": [
           "samHelper/jquery-1.10.2.js",
           "samHelper/samHelperChrExt.js",
           "SamTracker/govxArrow.js",
           "SamTracker/govxStateMachine.js",
           "SamTracker/samtracker.js",
           "samHelper/usdsJsHelper.js",
           "samHelper/samHelper.js"
           ]
	   }
  ],
  "minimum_chrome_version" : "6.0.0.0",
  "manifest_version": 2,
  "web_accessible_resources": [
      "samHelper/samHelper.html",
      "SamTracker/samtracker.html",
      "SamTracker/samscreen.png",
      "samHelper/samHelper.json"
  ]
}
